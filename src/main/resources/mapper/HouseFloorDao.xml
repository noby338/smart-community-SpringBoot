<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.woniuxy.smart_community.dao.HouseFloorDao">

    <insert id="insert">
        insert into house_floor( name,unit_id)
        values ( #{name}, #{unitId})
    </insert>
    <select id="selectById" resultType="com.woniuxy.smart_community.entity.HouseFloor">
        select * from house_floor where id=#{id}
    </select>
</mapper>