<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.woniuxy.smart_community.dao.HouseUnitDao">

    <insert id="insert">
        insert into house_unit (name, building_id) values (#{name},#{buildingId})
    </insert>

    <select id="selectById" resultType="com.woniuxy.smart_community.entity.HouseUnit">
        select * from house_unit where id=#{id}
    </select>
</mapper>